            "decoder_layer_sizes": [32, 256, 1024]
        },
        "conv_ae": {
            "allow_mlp_adjust": true,
            "encoder_no_feature_maps": [128, 64, 32, 16],
            "encoder_mlp_layer_sizes": [1024, 512, 128],
            "decoder_no_feature_maps": [16, 32, 64, 128],
            "decoder_mlp_layer_sizes": [128, 512, 1024],
